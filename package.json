{
  "name": "react-sight",
  "version": "1.0.6",
  "description": "Visualization tool for React Applications",
  "keywords": [
    "react",
    "fiber",
    "redux",
    "router",
    "chrome",
    "extension",
    "developer tool",
    "dev tool"
  ],
  "scripts": {
    "lint": "eslint chrome-ext",
    "start": "webpack -w",
    "start:dev": "NODE_PATH=. NODE_ENV='development' webpack-dev-server --content-base chrome-ext/build",
    "test": "jest",
    "coveralls": "NODE_PATH=. node_modules/.bin/jest && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build": "webpack -p",
    "build:debug": "NODE_ENV=debug webpack -w"
  },
  "repository": "git+https://github.com/React-Sight/React-Sight.git",
  "author": "David Sally, Grant Kang, & William He",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/React-Sight/React-Sight/issues"
  },
  "homepage": "https://github.com/React-Sight/React-Sight#readme",
  "dependencies": {
    "d3": "5.15.0",
    "d3-interpolate": "1.4.0",
    "json-formatter-js": "2.2.1",
    "lodash": "4.17.15"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/preset-env": "7.8.4",
    "babel-jest": "25.1.0",
    "copy-webpack-plugin": "5.1.1",
    "coveralls": "3.0.9",
    "css-loader": "3.4.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.20.1",
    "file-loader": "4.0.0",
    "image-webpack-loader": "5.0.0",
    "jest": "25.1.0",
    "node-sass": "4.13.1",
    "npm-run-all": "4.1.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.3"
  }
}
